<form action="" method="post">
  <div class="field">
    <label for="title">Title</label>
    <input type="text" name="title" id="title" />
  </div>

  <div class="field">
    <label for="description">Description</label>
    <textarea name="description" id="description">
</textarea>
  </div>

  <div class="field">
    <label for="url">Url</label>
    <textarea name="url" id="url">
</textarea>
  </div>

  <div class="field">
    <label for="phone">Phone</label>
    <input type="text" name="phone" id="phone" />
  </div>

  <div class="field">
    <label for="quantity">Quantity</label>
    <input type="number" name="quantity" id="quantity" />
  </div>

  <div class="field">
    <label for="float_number">Float number</label>
    <input type="text" name="float_number" id="float_number" />
  </div>

  <div class="field">
    <label for="decimal_number">Decimal number</label>
    <input type="text" name="decimal_number" id="decimal_number" />
  </div>

  <div class="field">
    <label for="full">Full</label>
    <input type="checkbox" name="full" id="full" value="1" />
  </div>

  <div class="field">
    <label for="email">Email</label>
    <input type="text" name="email" id="email" />
  </div>

  <div class="field">
    <label for="password">Password</label>
    <input type="text" name="password" id="password" />
  </div>

</form>
