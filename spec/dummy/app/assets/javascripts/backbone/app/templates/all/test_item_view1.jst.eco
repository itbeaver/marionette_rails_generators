<form action="" accept-charset="UTF-8" method="post"><input name="utf8" type="hidden" value="&#x2713;" />

<div class="field">
<label for="title">Title</label><br>
<input type="text" name="title" id="title" />
</div>

<div class="field">
<label for="description">Description</label><br>
<textarea name="description" id="description">
</textarea>
</div>

<div class="field">
<label for="url">Url</label><br>
<textarea name="url" id="url">
</textarea>
</div>

<div class="field">
<label for="phone">Phone</label><br>
<input type="text" name="phone" id="phone" />
</div>

<div class="field">
<label for="quantity">Quantity</label><br>
<input type="number" name="quantity" id="quantity" />
</div>

<div class="field">
<label for="float_number">Float number</label><br>
<input type="text" name="float_number" id="float_number" />
</div>

<div class="field">
<label for="decimal_number">Decimal number</label><br>
<input type="text" name="decimal_number" id="decimal_number" />
</div>

<div class="field">
<label for="full">Full</label><br>
<input type="checkbox" name="full" id="full" value="1" />
</div>

<div class="field">
<label for="email">Email</label><br>
<input type="text" name="email" id="email" />
</div>

<div class="field">
<label for="password">Password</label><br>
<input type="text" name="password" id="password" />
</div>

<div class="actions">
<input type="submit" name="commit" value="Save changes" />
</div>

</form>
